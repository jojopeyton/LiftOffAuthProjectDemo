@model List<AuthDemoProject.Models.Song>

<h1>Songs</h1>

@if (Model.Count == 0)
{
        <p>No songs listed</p>
}
else
{
        <div>
            <ul>
                @foreach(Song song in Model)
{
                        <li>
                            <a as-controller="Song" asp-action="About" asp-route-id="@song.Id">@song.Name</a>
                        </li>
}
            </ul>
        </div>
}
